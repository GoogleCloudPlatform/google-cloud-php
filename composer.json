    "homepage": "http://github.com/GoogleCloudPlatform/google-cloud-php",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Dave Supplee",
            "email": "dwsupplee@gmail.com"
        },
        {
            "name": "John Pedrie",
