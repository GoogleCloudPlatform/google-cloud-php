{
  "name": "google/gcloud",
  "type": "library",
  "description": "Google Cloud Client Library",
    "keywords": [
    "google apis client",
    "google api client",
    "google apis",
    "google api",
    "google",
    "google cloud platform",
    "google cloud",
    "cloud"
  ],
  "homepage": "http://github.com/GoogleCloudPlatform/gcloud-php",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Dave Supplee",
      "email": "dwsupplee@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.5",
    "rize/uri-template": "~0.3",
    "google/auth": "*",
    "guzzlehttp/guzzle": "6",
    "guzzlehttp/psr7": "*",
    "psr/http-message": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "4.8.*",
    "squizlabs/php_codesniffer": "2.*",
    "phpdocumentor/phpdocumentor": "*"
  },
  "autoload": {
    "psr-4": {
      "Google\\Cloud\\": "src"
    }
  }
}
