        "google apis client",
        "google api client",
        "google apis",
        "google api",
        "google",
        "google cloud platform",
        "google cloud",
        "cloud",
        "bigtable",
        "bigquery",
        "big query",
        "datastore",
        "stackdriver logging",
        "language",
        "natural language",
        "pubsub",
        "pub sub",
        "spanner",
        "speech",
        "storage",
        "gcs",
        "translate",
        "translation",
        "vision"
